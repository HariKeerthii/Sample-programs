#include <stdio.h>

int main()
{
   int arr[]= {1,2,3,2,1,0};
   
   for(int i=0;i<6;i++)
   {
       if(arr[i] > arr[i+1])
       continue;
       else if (arr[i]<arr[i+1])
       continue;
       else 
       {
         printf("Not mountain array \n");  
         break;
       }
       
   }
  
   return 0;
   
}
